<mvx:MvxWpfView
    x:Class="TimVinkemeier.AzureDevOpsToolkit.Views.RootView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:TimVinkemeier.AzureDevOpsToolkit.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:mvx="clr-namespace:MvvmCross.Platforms.Wpf.Views;assembly=MvvmCross.Platforms.Wpf"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <local:ViewOutlet
            x:Name="MainMenuOutlet"
            Grid.Column="0"
            Identifier="MainMenuOutlet" />
        <local:ViewOutlet
            x:Name="RouterOutlet"
            Grid.Column="1"
            Identifier="RouterOutlet" />
    </Grid>
</mvx:MvxWpfView>